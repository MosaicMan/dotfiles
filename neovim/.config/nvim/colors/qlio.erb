	### Variable types ####
	[ "Constant", "NONE", darkgray, "NONE" ],
	[ "String", "NONE", darkgray, "NONE" ],
	[ "StringDelimiter", "NONE", darkgray, "NONE" ],
	[ "Character", "NONE", darkgray, "NONE" ],
