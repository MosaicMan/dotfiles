	[ "Directory", "NONE", darkgray, "NONE" ],
	[ "Search", qlioGold, qlioDarkGray, "NONE" ],
	[ "IncSearch", "NONE", "NONE", "reverse" ],

	### Prompt/Status ####
	[ "StatusLine",qlioDarkGray, qlioWhite, "NONE" ],
